title UC5 - Class Diagram

class Company {
    string designation;
}

class Employee {
    String employeeID;
    String name;
    String address;
    String phoneNumber;
    String codeSOC;
}

class MedicalLabTechnician

class User {
  String name;
}

class Test {
    String code;
    String NHSCode;
    String description;
    String dateSample;
    String timeSmaple;
    String dateChemAnalysis;
    String timeChemAnalysis;
    String dateDiagnosis;
    String timeDiagnosis;
    String dateValidTest;
    String timeValidTest;
}

class Sample {
    String number_barcode;
}

class Client {
    String citizenCardNumber;
    String numberNHS;
    String numberTIN;
    String birthDate;
    string sex;
    String phoneNumber;
    string name;
}

class TestType {
    String designation;
    String code;
    String collectingMethods;
}

class Parameter {
    String designation;
    String code;
}

class ParameterCategory {
    String code;
    string description;
    String NHSID;
}

class Barcode
class ExternalModule

Company "1" -- "*" Employee: has >
Company "1" -- "*" Test: performs >
Company "1" -- "*" Client: has >
Company "1" -- "*" TestType: conducts >
Employee <|-- MedicalLabTechnician
Employee "0..1" -- "1" User: acts as >
MedicalLabTechnician "1" -- "*" Sample: records >
Test"1" -- "*" Sample: has >
Test "1" -- "1" TestType: is of >
Sample "1" -- "1" Barcode: contains >
Barcode "*" -- "1" ExternalModule : generated by >
Client "1" -- "*" Test: does a >
Client "*" -- "1" MedicalLabTechnician: is tested by >
Test "1" -- "1..*" Parameter: has >
Parameter "*" -- "1" ParameterCategory: presented under >
TestType "*" -- "*"  ParameterCategory: measure by >
